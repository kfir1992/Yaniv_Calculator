<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nykproduction.yanivcalculator"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="26" />

    <application
        android:name="com.nykproduction.yanivcalculator.App"
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.nykproduction.yanivcalculator.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.nykproduction.yanivcalculator.NewGame_Activity" />
        <activity
            android:name="com.nykproduction.yanivcalculator.About_Activity"
            android:theme="@style/transparent" />
        <activity android:name="com.nykproduction.yanivcalculator.ADD_PLAYER" />
        <activity android:name="com.nykproduction.yanivcalculator.Game_Play" />
        <activity android:name="com.nykproduction.yanivcalculator.Enter_Score" />

        <service
            android:name="io.objectbox.android.AndroidObjectBrowserService"
            android:exported="false" />

        <meta-data
            android:name="android.support.VERSION"
            android:value="26.1.0" />
        <meta-data
            android:name="android.arch.lifecycle.VERSION"
            android:value="27.0.0-SNAPSHOT" />
    </application>

</manifest>